{
  "name": "@vaiton86/argo-api",
  "version": "1.5.5",
  "description": "An API to interact with Argo ScuolaNext backend.",
  "main": "./lib/index.js",
  "author": "VaiTon86",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint test"
    }
  },
  "scripts": {
    "compile": "tsc -p .",
    "install": "npm run compile",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.css,.scss}\"",
    "lint": "tslint --project .",
    "test": "cross-env JASMINE_CONFIG_PATH=jasmine.json ts-node node_modules/jasmine/bin/jasmine"
  },
  "dependencies": {
    "await-to-js": "^2.1.1",
    "axios": "^0.16.2",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "jasmine": "^3.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0"
  }
}
